<h1>CURRENT TASKS</h1>

<table>
<tr>
  <th> Name </th>
  <th> Description </th>
  <th> Date Completed </th>
</tr>

<% if @current_task_display %>
    <% @current_task_display.each do |task| %>
    <div>
      <tr>
        <td> <%= task[1] %> </td>
        <td> <%= task[2] %> </td>
        <td> <%= task[3] %> </td>
        <td>
          <input type="button" name="Delete" value="Delete">
        </td>
      </tr>
    </div>

    <% end %>
<%end%>

</table>
